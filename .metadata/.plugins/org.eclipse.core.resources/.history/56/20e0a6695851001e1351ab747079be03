
public class Animal {
	void makesound() 
	{
		System.out.println("Animal Barks");
	}
class Dog extends Animal
{
	void makesound() 
	{
		System.out.println("Dog Barks");
	}
}
public static void main(String [] args) 
{
	Animal animal = new Animal();
	Animal animal = new animal.dog()
	animal.makesound();
}
}
