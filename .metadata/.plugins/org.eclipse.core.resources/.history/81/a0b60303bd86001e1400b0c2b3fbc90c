class Runner extends Thread
{
	public void run()
	{		
		for(int i=0;i<10;i++) 
		{
			System.out.println("Hi");
			try 
			{
				Thread.sleep(300);
			} 
			catch (InterruptedException e) 
			{
				e.getMessage();
			}
		}
	}
}

public class BasicMultiThreading 
{
	public static void main(String [] args) 
	{
		Runner run = new Runner();
		
	}
}
